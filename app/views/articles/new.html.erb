
<div class="container">
<h1 class="text-center">Create new article</h1>
  <div class="row">
    <%= form_for(@article) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title, class: "form-control" %>
    <%= form.label :content %>
    <%= form.text_field :content, class: "form-control" %>
    <br>
    <%= form.submit class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

