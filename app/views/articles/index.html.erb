
<div class="container">
  <h1>Articles</h1>
  <% @articles.each do |article| %>
  <div>
    <ul>
      <li><%= article.title %></li>
      <%= article.content %>

      <br>
      <%= link_to "Show details", article_path(article) %>
      <%= link_to "Edit", edit_article_path(article) %>
      <%= link_to "Delete", article_path(article), method: :delete %>
    </ul>
  </div>
  <% end %>

  <%= link_to "Create new article", new_article_path %>

</div>

